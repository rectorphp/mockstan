{
    "name": "rector/mockstan",
    "type": "phpstan-extension",
    "description": "Set of Symplify rules for PHPStan",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "phpstan/phpstan": "^2.1.38"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Mockstan\\": "src"
        },
        "files": [
            "src/functions/fast-functions.php"
        ]
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config/services/services.neon"
            ]
        }
    }
}
